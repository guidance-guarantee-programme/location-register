<div class="row">
  <div class="col-md-12">
    <h1 class="inline-block">Edit Location - <%= @location.title %></h1>
    <%= render 'admin/locations/labels', location: @location %>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <%= form_for @location, url: admin_location_path, method: 'put' do |f| %>
      <%= render 'form', f: f, location: @location %>
    <% end %>
  </div>
</div>
